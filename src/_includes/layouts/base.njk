---
type: page
eleventyComputed:
  meta:
    site:
      name: "{{ site.title | safe }}"
      description: "{{ site.description | safe }}"
      url: "{{ site.url }}"
      logo:
        src: "{{ site.url  + '/' +  site.logo }}"
        width: 400
        height: 497
    breadcrumbs:
      - name: Accueil
        url: "{{ site.url }}"
        position: 1
        image: "{{ site.url  + '/' +  site.header_image }}"
    language: fr-FR
    url: "{{ site.url  +  page.url }}"
    title: "{{ page.title | safe }}"
    description: "{{ page.subtitle | safe }}"
    image:
      src: "{{ site.url  + '/' +  site.header_image }}"
    organization:
      name: 11ty boilerplate
      legalName: 11ty boilerplate
      url: https://www.11ty-boilerplate.fr
      logo: "{{ site.url  + '/' +  site.logo }}"
      description: "{{ site.description | safe }}"
      foundingDate: '2020'
      founders:
      - name: Owner name
      address:
        streetAddress: owner adress
        addressLocality: owner town
        postalCode: '88888888'
        addressCountry: France
      contactPoint:
      - telephone: "phone"
        contactType: Service Client
        areaServed: FR
        availableLanguage:
        - English
        - French
      - email: "john@doe.com"
      sameAs:
      - https://www.facebook.com/johndoe/
      - https://twitter.com/johndoe
      - https://www.instagram.com/johndoe/
      - https://www.google.com/search?gs_ssp=JOHNDOE
      - https://www.linkedin.com/company/johndoe
---
  
<!DOCTYPE html>
<html lang="fr">

<head>
<meta charset="UTF-8">

  {% jsonLdScript meta, type, tags %}


    {% include "partials/structure/site-head.njk" %}

    {%- block seo -%}
    
    {% set titletag = [ title, " - " , titleshort] | join %}
    <title>  {{ titletag or title }}</title>
    <!--Twitter Card-->
    <meta name="twitter:title" content="{{ site.title }}">
    <meta name="twitter:description" content="{{ subtitle or site.description }}">
    
    <!--Schema-->
    <meta itemprop="name" content="{{ site.title}}">
    <meta name="description" content="{{ subtitle or page.meta.description or site.description }}">
    
    <!-- Facebook OpenGraph -->
    <meta property="og:title" content="{{ site.title}}">
    <meta property="og:description" content="{{ subtitle or site.description }}">

    {%- endblock -%}
  
</head>

<!-- Matomo tracking analytics -->
<script>
  var _paq = window._paq = window._paq || [];
  /* tracker methods like "setCustomDimension" should be called before "trackPageView" */
  _paq.push(['trackPageView']);
  _paq.push(['enableLinkTracking']);
  (function() {
    var u="//analytics.6337.fr/";
    _paq.push(['setTrackerUrl', u+'matomo.php']);
    _paq.push(['setSiteId', '1']);
    var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
    g.async=true; g.src=u+'matomo.js'; s.parentNode.insertBefore(g,s);
  })();
</script>
<!-- End Matomo Code -->

  <body>

    {% include "partials/structure/site-header.njk" %}
    <main>
      {{ content | safe }}
    </main>
    {% include "partials/structure/site-foot.njk" %}
  </body>
</html>
